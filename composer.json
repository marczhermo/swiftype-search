{
    "name": "marczhermo/swiftype-search",
    "description": "Swiftype SilverStripe Module For Search",
    "type": "silverstripe-module",
    "license": "BSD-3-Clause",
    "keywords": [
        "swiftype",
        "silverstripe",
        "search",
        "admin"
    ],
    "require": {
        "marczhermo/search-list": "^0.1",
        "guzzlehttp/ringphp": "^1.1",
        "silverstripe/queuedjobs": "^2"
    },
    "require-dev": {
        "phpunit/phpunit": "^3 || ^4 || ^5"
    },
    "extra": {
        "expose": [
            "client/dist",
            "client/lang"
        ]
    },
    "autoload": {
        "psr-4": {
            "Marcz\\Swiftype\\": "src/",
            "Marcz\\Swiftype\\Tests\\": "tests/php/"
        }
    },
    "scripts": {
        "lint": "php phpcs.phar src/ tests/php/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
